#!/bin/python3

import math
import os
import random
import re
import sys
#max index is [5][5]
def hourGlass(arr):
    maxSum = []
    for i in range(4):
        for j in range(4):
            temp = (arr[i][j]+arr[i][j+1]+arr[i][j+2] + 
                    arr[i+1][j+1]+
                    arr[i+2][j]+arr[i+2][j+1]+arr[i+2][j+2])
            maxSum.append(temp)
    return max(maxSum)
if __name__ == '__main__':
    arr = []

    for _ in range(6):
        arr.append(list(map(int, input().rstrip().split())))

print(hourGlass(arr))
